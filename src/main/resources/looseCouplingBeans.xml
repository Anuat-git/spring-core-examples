<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id ="missedCall" class="com.diloose.coupling.MissedCallService"></bean>
    <bean id ="helloTune" class="com.diloose.coupling.HelloTuneService"></bean>
    <bean id ="dataService" class="com.diloose.coupling.DataService"></bean>

    <bean id="sim" class="com.diloose.coupling.Sprint">
        <property name="service" ref="dataService"></property>
    </bean>

</beans>